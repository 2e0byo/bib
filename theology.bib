@article{10.2307/3710542,
  abstract =	 {Combining the points of view of the sociology of
                  knowledge and the sociology of religion, Max Weber
                  posed the question as to which classes are likely to
                  develop a genuinely religious outlook, and which
                  not. In the latter category he counted the peasants,
                  the aristocrats, the bureaucrats and the traders; in
                  the former the artisans and the intellectuals,
                  especially the "pariah" or "proletaroid"
                  intellectuals. The paper endeavours to show that
                  this analysis suffers from serious shortcomings, and
                  suggests that Tönnies' dichotomy of community-type
                  and associational societies provides a better
                  insight into the origins of relatively religious and
                  relatively irreligious world-views than does Weber's
                  distinctions.},
  author =	 {Werner Stark},
  issn =	 00380210,
  journal =	 {Sociological Analysis},
