@article{10.2307/44409632,
 abstract = {L'auteur précise d'abord la notion de « réception »
en la prenant cependant dans une large extension. La
« réception » est un fait que l'auteur suit dans
l'histoire des conciles, de la liturgie, du
droit. Il passe ensuite en revue les théories
proposées pour en rendre compte : acceptation des
lois, intention du législateur de ne pas obliger les
sujets sans leur consentement, conception liant la
légitimité du pouvoir à son « utilité ». On propose
enfin sa justification: la réception ne crée pas le
droit qu'a l'autorité de décider, mais elle déclare
que l'Église reconnaît son bien dans la décision
portée, et ainsi elle achève celle-ci dans la ligne
de son efficacité.},
 author = {Yves Congar},
 journal = {Revue des Sciences philosophiques et théologiques},
 keywords = {dissertation_primary},
 language = {french},
 number = {3},
 pages = {369--403},
 publisher = {Librairie Philosophique J. Vrin},
 title = {La « réception » comme réalité ecclésiologique},
