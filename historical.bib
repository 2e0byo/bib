@article{fouilloux12:_premie_saulchoir,
 abstract = {L'ouverture des archives du Saint-Office pour le pontificat de Pie XI montre qu'une première alerte a touché le couvent d'études du Saulchoir en 1932. Dénoncé par deux frères étudiants, il a été l'objet d'une enquête dont les réponses apportent une vive lumière sur les divergences au sein de la « grande famille des thomistes ». Cette enquête aboutit à un non-lieu au moment précis où le Saulchoir reçoit le chapitre général de l'Ordre qui confère la maîtrise en théologie à l'un des professeurs suspectés, le père Marie-Dominique Chenu, qui en devient aussitôt régent. The opening of the Holy Office's archives, under Pius XI's pontificate, shows that a first warning was issued against the Saulchoir House of Studies in 1932. Denounced by two student brothers, it became the target of an investigation, the answers to which shed light on the divergences at the heart of the "great thomist family". This investigation led to a mistrial exactly at the time when the Saulchoir received the visit of the General Chapter of the Order who granted one of the suspected professors, Father Marie-Dominique Chenu, a Master's degree in theology, who was thus immediately appointed the House rector.},
 author = {Étienne Fouilloux},
 issn = {00352209, 21184445},
 journal = {Revue des Sciences philosophiques et théologiques},
 number = {1},
 pages = {93--105},
 pdf = {/home/john/phd/library/pdfs/premier-alerte-sur-saulchoir.pdf},
 publisher = {Vrin},
 title = {Première alerte sur le saulchoir (1932)},
 url = {http://www.jstor.org/stable/44626786},
 urldate = {2022-12-26},
 volume = {96},
 year = {2012}
}

