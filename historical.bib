@article{cholvy11:_une_controverse,
 abstract = {Revenant sur la controverse qui eut lieu dans la théologie française au milieu du XXe siècle, l'article propose une nouvelle contextualisation en différencial les thématiques de la controverse et en se concentrant sur les débats de Lubac avec ses amis jésuites. De ces débats, il appaire que la distinction entre sumaturel et grace est fondamentale dans la théologie de Lubac et permet seule de sortir de confusions durables. C'est sur cette thèse de fond que Lubac construisit sa théologie du sumaturel, qui le conduisit à se ressaisir des thèmes de la gratuite et de la finalité et à approfondir les affirmations fondamentales de la doctrine de la Création. The article harks back to a controversy that took place in French theology in the middle of the 20th century, and offers a new contextualisation by distinguishing among the various elements of the controversy and concentrating on the debates between de Lubac and his Jesuit friends. It is clear from these debates that the distinction between grace and the supernatural is fundamental in de Lubac's theology, and that only by making this distinction can theology move beyond some lasting misunderstandings. De Lubac constructed his theology of the supernatural on this fundamental thesis, which led him to take up the themes of gratuity and finality in a new way, and to deepen the underlying affirmations made in the doctrine of Creation.},
 author = {Brigitte Cholvy},
 issn = {00174114},
 journal = {Gregorianum},
 number = {4},
 pages = {797--827},
 pdf = {/home/john/phd/library/pdfs/brigitte_cholvy-une_controverse_majeure_henri_de_lubac_et_le_surnaturel.pdf},
 publisher = {GBPress- Gregorian Biblical Press},
 title = {Une controverse majeure: Henri de Lubac et le surnaturel},
 url = {http://www.jstor.org/stable/43922460},
 urldate = {2022-12-26},
 volume = {92},
 year = {2011}
}

